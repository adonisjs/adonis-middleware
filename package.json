{
  "name": "adonis-middleware",
  "version": "1.0.8",
  "description": "List of official middleware for adonis-framework",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && istanbul cover _mocha --report lcovonly -- -R spec test && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "coverage": "istanbul cover _mocha test --bail",
    "lint": "standard src/**/*.js test/*.js"
  },
  "standard": {
    "global": ["use", "it", "describe", "context", "before", "after"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-middleware.git"
  },
  "keywords": [
    "adonis",
    "http-middleware",
    "middleware",
    "adonisjs"
  ],
  "author": "adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-middleware/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-middleware#readme",
  "dependencies": {
    "bytes": "^2.4.0",
    "co-body": "^4.2.0",
    "csrf": "^3.0.3",
    "formidable": "^1.0.17",
    "lodash": "^4.14.1",
    "node-csp": "^1.0.1",
    "node-exceptions": "^1.0.1",
    "node-guard": "^1.0.0",
    "node-uuid": "^1.4.7"
  },
  "devDependencies": {
    "adonis-fold": "^3.0.2",
    "chai": "^3.5.0",
    "co": "^4.6.0",
    "co-mocha": "^1.1.3",
    "coveralls": "^2.11.12",
    "cz-conventional-changelog": "^1.1.6",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.0",
    "mocha-lcov-reporter": "^1.2.0",
    "standard": "^7.1.2",
    "supertest": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
